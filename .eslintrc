{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-undef": 2,
    "object-curly-newline": 0,
    "object-curly-spacing": ["error", "always"],
    "arrow-parens": ["error", "as-needed"],
    "semi": ["error", "always"]
  }
}
